<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.asus.freingo.activities.TabActivity">

    <android.support.v4.view.ViewPager
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_marginBottom="56dp"
        android:id="@+id/contentviewpager"/>



    <eu.long1.spacetablayout.SpaceTabLayout
        android:layout_height="wrap_content" android:layout_width="match_parent"
        android:id="@+id/spaceTabLayout"
        app:icon_five="@drawable/ic_notifications_black_24dp"
        app:icon_four="@drawable/ic_stars_black_24dp"
        app:icon_three="@drawable/ic_place_black_24dp"
        app:icon_two="@drawable/ic_account_circle_black_24dp"
        app:icon_one="@drawable/ic_home_black_24dp"
        app:text_color="#61FFFFFF"
        app:button_color="#ff9d4c"
        app:tab_color="@color/colorPrimaryDark"
        app:starting_position="one"
        app:number_of_tabs="five"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
    </eu.long1.spacetablayout.SpaceTabLayout>
</RelativeLayout>
